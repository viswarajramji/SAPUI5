<mvc:View xmlns:filterbar="sap.ui.comp.filterbar" xmlns:cards="sap.f.cards" xmlns:f="sap.f" xmlns:core="sap.ui.core" xmlns:layout="sap.ui.layout"
    controllerName="actions.controller.Master"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
>
	<Shell id="shell">
		<App id="app">
			<pages>
					<layout:VerticalLayout
						width="100%"
						
					>
						<layout:content>
							<filterbar:FilterBar showClearButton="true" considerGroupTitle="true" >
								
								<filterbar:filterGroupItems>
									<filterbar:FilterGroupItem groupName="__$INTERNAL$" name="A" label="Delivery Date" partOfCurrentVariant="true" visibleInFilterBar="true">
										<filterbar:control>
											<DatePicker ></DatePicker>
										</filterbar:control>
									</filterbar:FilterGroupItem>
									<filterbar:FilterGroupItem groupName="__$INTERNAL$" name="B" label="Available Date" partOfCurrentVariant="true" visibleInFilterBar="false">
										<filterbar:control>
											<DatePicker ></DatePicker>
										</filterbar:control>
									</filterbar:FilterGroupItem>
									<filterbar:FilterGroupItem groupName="__$INTERNAL$" name="C" label="Proccessed Date" partOfCurrentVariant="true" visibleInFilterBar="false">
										<filterbar:control>
											<DatePicker ></DatePicker>
										</filterbar:control>
									</filterbar:FilterGroupItem>
								</filterbar:filterGroupItems>
							</filterbar:FilterBar>
						</layout:content>
					</layout:VerticalLayout>
			</pages>
		</App>
	</Shell>
</mvc:View>
