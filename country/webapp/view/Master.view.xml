<mvc:View xmlns:f="sap.f"
	controllerName="country.controller.Master"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m"
	xmlns:c="sap.ui.core">
	<f:DynamicPage toggleHeaderOnTitleClick="false" showFooter="true" >
		<f:content>
			<Table id="masterTable" 
			items="{
					path: 'country>/countryList',
					sorter: {
						path: 'countryName'
					}
				}"
				class="sapFDynamicPageAlignContent">
				<columns>
					 <Column>
						 	<Text text="Hello"></Text>
					</Column>
				</columns>
				<items>
					<ColumnListItem type="Navigation" press="countryDetails">
						 <cells>
							<ObjectIdentifier title="{country>countryCode}" ></ObjectIdentifier>	
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</f:content>
	</f:DynamicPage>
</mvc:View>

